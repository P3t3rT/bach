{% extends "layout.html.twig" %}

{% block title %}Blank page | overzicht{% endblock %}
{% block pagetitle %}Blank page layout<small> Blank page</small>{% endblock pagetitle %}

{% block stylesheets %}
    {{ parent() }}
{% endblock stylesheets %}

{% block content %}
    {{ include("flash.html.twig") }}

    <!-- BEGIN PAGE CONTENT INNER -->
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                <tr class="heading">
                    <th>
                        Type
                    </th>
                    <th>
                        Highlight
                    </th>
                </tr>
                </thead>
                <tbody>
                {% for row in rows %}
                    <tr>
                        <td>{{ row['type'] }}</td>
                        <td>{{ row.highlight|raw }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
    <!-- END PAGE CONTENT INNER -->
{% endblock content%}

{#<!-- BEGIN JAVASCRIPTS -->#}
{% block javascripts %}
    {{ parent() }}
{% endblock javascripts%}

{% block jquery %}
    {{ parent() }}
	<script>
      jQuery(document).ready(function() {
         Metronic.init(); // init metronic core components
         Layout.init(); // init current layout
      });
   </script>
{% endblock jquery %}
{#<!-- END JAVASCRIPTS -->#}
