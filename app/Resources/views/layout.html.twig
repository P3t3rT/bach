<!DOCTYPE html>
{% block head %}
<!--
Template: TISDefault
Template Name: Metronic - Responsive Admin Dashboard Template build with Twitter Bootstrap 3.3.1
Version: 3.6.1
Author: KeenThemes
-->
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
{% endblock head %}

{% block meta %}
<!-- BEGIN HEAD -->
<head>
<meta charset="utf-8"/>
<title>{% block title %} Layout - Block title{% endblock title %}</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<meta http-equiv="Content-type" content="text/html; charset=utf-8">
<meta content="" name="description"/>
<meta content="" name="author"/>
{% endblock meta%}

{% block stylesheets %}
<link href="{{ app.request.getScheme() }}://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css"/>
	{% stylesheets
		'assets/global/plugins/font-awesome/css/font-awesome.min.css'
		'assets/global/plugins/simple-line-icons/simple-line-icons.min.css'
		'assets/global/plugins/bootstrap/css/bootstrap.min.css'
		'assets/global/plugins/uniform/css/uniform.default.css'
		'assets/global/css/plugins.css'
		'assets/admin/layout3/css/layout.css'
		'assets/admin/layout3/css/custom.css'
		'assets/global/css/components-rounded.css'
		'assets/admin/layout3/css/themes/default.css'
		filter='cssrewrite'
		%}
		<link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
	{% endstylesheets %}
{% endblock stylesheets %}
	<link rel="shortcut icon" href="{{ asset('favicon.ico') }}"/>
</head>
<!-- END HEAD -->

{% block body %}

{% block header %}

<!-- BEGIN BODY -->
<!-- DOC: Apply "page-header-menu-fixed" class to set the mega menu fixed  -->
<!-- DOC: Apply "page-header-top-fixed" class to set the top menu fixed  -->
<body>
<!-- BEGIN HEADER -->
<div class="page-header">
	<!-- BEGIN HEADER TOP -->
	<div class="page-header-top">
		<div class="container">
			<!-- BEGIN LOGO -->
			<div class="page-logo">
				<a href="{{ path('bach_list') }}">
				{% image 'assets/custom/img/bach2.png' %}
				<img src="{{ asset_url }}" alt="logo"  class="bach-logo img-responsive"/>
				{% endimage %}
				</a>
			</div>
			<!-- END LOGO -->
			<!-- BEGIN RESPONSIVE MENU TOGGLER -->
			<a href="javascript:;" class="menu-toggler"></a>
			<!-- END RESPONSIVE MENU TOGGLER -->
			<!-- BEGIN TOP NAVIGATION MENU -->
			<div class="top-menu">
				<ul class="nav navbar-nav pull-right">
					<!-- BEGIN USER LOGIN DROPDOWN -->
					<li class="dropdown dropdown-user dropdown-dark">
						<!-- DOC: Remove data-hover="dropdown" and data-close-others="true" attributes below to disable the dropdown opening on mouse hover -->
						<a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
						{% image 'assets/admin/layout3/img/avatar.png'%}
							<img class="img-circle" src="{{ asset_url }}" alt=""/>
						{% endimage %}
						<span class="username username-hide-mobile">Guest</span>
						</a>
						<ul class="dropdown-menu dropdown-menu-default">
							<li>
								{#<a href="extra_profile.html">#}
								<a href="#">
								<i class="icon-user"></i> My Profile </a>
							</li>
							<li class="divider">
							</li>
							<li>
								{#<a href="login.html">#}
								<a href="#">
								<i class="icon-key"></i> Log Out </a>
							</li>
						</ul>
					</li>
					<!-- END USER LOGIN DROPDOWN -->
				</ul>
			</div>
			<!-- END TOP NAVIGATION MENU -->
		</div>
	</div>
	<!-- END HEADER TOP -->
	<!-- BEGIN HEADER MENU -->
	<div class="page-header-menu">
		<div class="container">
			<!-- BEGIN HEADER SEARCH BOX -->
			<form class="search-form" action="{{ path('bach_search') }}" method="GET">
				<div class="input-group">
					<input type="text" class="form-control" placeholder="Search" name="query">
					<span class="input-group-btn">
					<a href="javascript:;" class="btn submit"><i class="icon-magnifier"></i></a>
					{#<a href="{{ path('bach_search') }}" class="btn submit"><i class="icon-magnifier"></i></a>#}
					</span>
				</div>
			</form>
			<!-- END HEADER SEARCH BOX -->
			<!-- BEGIN MEGA MENU -->
			<!-- DOC: Apply "hor-menu-light" class after the "hor-menu" class below to have a horizontal menu with white background -->
			<!-- DOC: Remove data-hover="dropdown" and data-close-others="true" attributes below to disable the dropdown opening on mouse hover -->
			<div class="hor-menu">
				<ul class="nav navbar-nav">
					<li>
						<a href="{{ path('bach_list') }}">Home</a>
					</li>
					<li>
						<a href="{{ path('bach_list') }}">Links</a>
					</li>
				</ul>
			</div>
			<!-- END MEGA MENU -->
		</div>
	</div>
	<!-- END HEADER MENU -->
</div>
<!-- END HEADER -->
<!-- BEGIN PAGE CONTAINER -->
<div class="page-container">
	<!-- BEGIN PAGE HEAD -->
	<div class="page-head">
		<div class="container">
			<!-- BEGIN PAGE TITLE -->
			<div class="page-title">
				<h1>
				{% block pagetitle %}Blank Page <small>blank page</small>{% endblock pagetitle %}
				</h1>
			</div>
			<!-- END PAGE TITLE -->
		</div>
	</div>
	<!-- END PAGE HEAD -->
	<!-- BEGIN PAGE CONTENT -->
	<div class="page-content">
		<div class="container">
{% endblock header%}

	<!-- BEGIN PAGE CONTENT INNER -->
	{% block content %}
	   {% block fos_user_content %}{% endblock fos_user_content %}
	{% endblock content%}
	<!-- END PAGE CONTENT INNER -->

		</div>
	</div>
	<!-- END PAGE CONTENT -->
</div>
<!-- END PAGE CONTAINER -->

<!-- BEGIN PRE-FOOTER -->
<div class="page-prefooter">
	<div class="container">
		<div class="row">
			<div class="col-md-3 col-sm-6 col-xs-12 footer-block">
				<h2>About</h2>
				<p>
                    This site has two purposes:<br>
                    1. Find music in my audio collection<br>
                    2. It is my Symfony2 playground
				</p>
			</div>
			<div class="col-md-3 col-sm-6 col-xs-12 footer-block">
				<h2>Contact</h2>
				<address class="margin-bottom-40">
				Phone: +31 653 658 686<br>
				 Email: <a href="mailto:peter.termaten@gmail.com">peter.termaten@gmail.com</a>
				</address>
			</div>
		</div>
	</div>
</div>
<!-- END PRE-FOOTER -->
<!-- BEGIN FOOTER -->
<div class="page-footer">
	<div class="container">
        2015 - {{ "now"|date("Y") }} &copy; Peter Termaten. All Rights Reserved.
	</div>
</div>
<div class="scroll-to-top">
	<i class="icon-arrow-up"></i>
</div>
<!-- END FOOTER -->
{% endblock body%}

<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
<!-- BEGIN CORE PLUGINS -->
{% block javascripts %}
<!--[if lt IE 9]>
	{% javascripts output='js/compiled'
	   'assets/global/plugins/respond.min.js'
	   'assets/global/plugins/excanvas.min.js'
	%}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
<![endif]-->

	{% javascripts
	     'assets/global/plugins/jquery.min.js'
	     'assets/global/plugins/jquery-migrate.min.js'
	     'assets/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js'
	     'assets/global/plugins/bootstrap/js/bootstrap.min.js'
	     'assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js'
	     'assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js'
	     'assets/global/plugins/jquery.blockui.min.js'
	     'assets/global/plugins/jquery.cokie.min.js'
	     'assets/global/plugins/uniform/jquery.uniform.min.js'
	     'assets/global/scripts/metronic.js'
		 'assets/admin/layout3/scripts/layout.js'
	 %}
	 <script src="{{ asset_url }}" type="text/javascript"></script>
	 {% endjavascripts %}
{% endblock javascripts%}

{% block jquery %}
		<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
	    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
{% endblock jquery %}
<!-- END JAVASCRIPTS -->

</body>
<!-- END BODY -->
</html>
